{
  "name": "vault-manager",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "mocha ./test -name '*.spec.js' --recursive --exit",
    "test:watch": "npm test -- --watch",
    "depcheck": "depcheck",
    "stylecheck": "eslint src/**/*.js test/**/*.js",
    "check": "npm audit && npm run stylecheck && npm run depcheck && npm outdated",
    "coverage": "nyc -- npm test"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.11",
    "morgan": "~1.9.0",
    "pg": "^7.8.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "depcheck": "^0.6.11",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  }
}
